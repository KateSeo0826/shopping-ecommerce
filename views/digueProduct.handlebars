<div class="container">
  <div class="container">
     {{#each digProduct}}
    <div class="col mb-4">
      <div class="card">
        <img src="/uploads/{{proPic}}" class="card-img-top">
        <div class="card-body">
          <h5 class="card-title"> <b>{{this.pName}}</b>
          <p class="card-text">CND ${{this.pPrice}}</p>
            <button class = "btn"><a href="/prodetails/{{this.id}}"><i class="fa fa-info-circle">Details</i></a></button>
        </div>
      </div>
    </div>
     {{/each}}
  </div>
</div>
